{
  "mcpServers": {
    "github": {
      "transport": "http",
      "url": "https://github-mcp.fly.dev/mcp"
    },
    "filesystem": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."],
      "env": {}
    },
    "git": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git"],
      "env": {}
    },
    "playwright": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "playwright-mcp"],
      "env": {}
    },
    "fetch": {
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-fetch"],
      "env": {}
    }
  }
}
