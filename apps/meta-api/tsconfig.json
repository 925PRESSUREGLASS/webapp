{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "..",
    "outDir": "./dist"
  },
  "include": ["src/**/*.ts", "../domain/**/*.ts"],
  "exclude": ["node_modules"]
}
