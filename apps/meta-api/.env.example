# Example database URL (for Prisma/Postgres)
DATABASE_URL=postgresql://user:password@localhost:5432/metabuild

# Optional API key required by meta-api (dashboard will send x-api-key if set)
API_KEY=

# Optional CORS allowlist (comma separate or single origin); defaults to *
ALLOWED_ORIGIN=http://localhost:5173

# Optional rate limit per minute (per IP). Leave empty to disable.
RATE_LIMIT_PER_MIN=120
